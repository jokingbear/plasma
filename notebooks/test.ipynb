    "import plasma.functional as F\n",
    "\n",
    "\n",
    "class A(F.AutoPipe):\n",
    "\n",
    "    def run(self, x):\n",
    "        return x + 5\n",
    "    \n",
    "\n",
    "def logger(inputs, outputs):\n",
    "    print('inputs', inputs)\n",
    "    print('outputs', outputs)\n",
    "\n",
    "a = A()\n",
    "\n",
    "a.add_logger(logger)\n",
    "\n",
    "a.run(5)"
